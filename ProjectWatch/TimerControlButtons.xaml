                                <EasingColorKeyFrame KeyTime="0" Value="#FFFAD006"/>
                                <EasingColorKeyFrame KeyTime="0:0:3" Value="#FF7C7B78"/>
                                <EasingColorKeyFrame KeyTime="0:0:6" Value="#FFFAD006"/>
